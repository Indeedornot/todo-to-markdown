{
	"arrowParens": "always",
	"bracketSameLine": true,
	"bracketSpacing": false,
	"embeddedLanguageFormatting": "auto",
	"insertPragma": false,
	"printWidth": 120,
	"proseWrap": "preserve",
	"quoteProps": "as-needed",
	"requirePragma": false,
	"semi": true,
	"singleQuote": true,
	"tabWidth": 2,
	"trailingComma": "es5",
	"useTabs": true,
	"endOfLine": "crlf"
}
